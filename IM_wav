S_0x5a7a81e21090 .scope module, "IM_tb" "IM_tb" 2 1;
 .timescale 0 0;
v0x5a7a81e35a20_0 .var "Halt", 0 0;
v0x5a7a81e35b10_0 .var "address", 4 0;
v0x5a7a81e35be0_0 .var "clk", 0 0;
v0x5a7a81e35ce0_0 .net "instruction_out", 7 0, v0x5a7a81e357b0_0;  1 drivers
S_0x5a7a81e21220 .scope module, "u_IM" "IM" 2 7, 3 1 0, S_0x5a7a81e21090;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "Halt";
    .port_info 2 /INPUT 5 "address";
    .port_info 3 /OUTPUT 8 "instruction_out";
P_0x5a7a81e0cc30 .param/l "ADDRESS_WIDTH" 0 3 8, +C4<00000000000000000000000000000101>;
P_0x5a7a81e0cc70 .param/l "DATA_WIDTH" 0 3 7, +C4<00000000000000000000000000001000>;
P_0x5a7a81e0ccb0 .param/l "MEMORY_DEPTH" 0 3 9, +C4<00000000000000000000000000100000>;
v0x5a7a81e0c270_0 .net "Halt", 0 0, v0x5a7a81e35a20_0;  1 drivers
v0x5a7a81e35540_0 .net "address", 4 0, v0x5a7a81e35b10_0;  1 drivers
v0x5a7a81e35620_0 .net "clk", 0 0, v0x5a7a81e35be0_0;  1 drivers
v0x5a7a81e356f0_0 .var "hltSignal", 0 0;
v0x5a7a81e357b0_0 .var "instruction_out", 7 0;
v0x5a7a81e358e0 .array "instructions", 0 31, 7 0;
E_0x5a7a81de6090 .event posedge, v0x5a7a81e35620_0;
    .scope S_0x5a7a81e21220;
T_0 ;
    %vpi_call 3 19 "$readmemb", "./instruction.mem", v0x5a7a81e358e0, 32'sb00000000000000000000000000000000, 32'sb00000000000000000000000000011111 {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x5a7a81e21220;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a7a81e356f0_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x5a7a81e21220;
T_2 ;
    %wait E_0x5a7a81de6090;
    %load/vec4 v0x5a7a81e0c270_0;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_2.0, 4;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5a7a81e356f0_0, 0, 1;
T_2.0 ;
    %jmp T_2;
    .thread T_2;
    .scope S_0x5a7a81e21220;
T_3 ;
    %wait E_0x5a7a81de6090;
    %load/vec4 v0x5a7a81e356f0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_3.0, 8;
    %load/vec4 v0x5a7a81e35540_0;
    %pad/u 7;
    %ix/vec4 4;
    %load/vec4a v0x5a7a81e358e0, 4;
    %store/vec4 v0x5a7a81e357b0_0, 0, 8;
    %jmp T_3.1;
T_3.0 ;
    %load/vec4 v0x5a7a81e357b0_0;
    %store/vec4 v0x5a7a81e357b0_0, 0, 8;
T_3.1 ;
    %jmp T_3;
    .thread T_3;
    .scope S_0x5a7a81e21090;
T_4 ;
    %vpi_call 2 15 "$dumpfile", "IM.vcd" {0 0 0};
    %vpi_call 2 16 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5a7a81e21090 {0 0 0};
    %end;
    .thread T_4;
    .scope S_0x5a7a81e21090;
T_5 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a7a81e35be0_0, 0, 1;
T_5.0 ;
    %delay 25, 0;
    %load/vec4 v0x5a7a81e35be0_0;
    %inv;
    %store/vec4 v0x5a7a81e35be0_0, 0, 1;
    %jmp T_5.0;
    %end;
    .thread T_5;
    .scope S_0x5a7a81e21090;
T_6 ;
    %pushi/vec4 0, 0, 5;
    %store/vec4 v0x5a7a81e35b10_0, 0, 5;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a7a81e35a20_0, 0, 1;
    %delay 50, 0;
    %pushi/vec4 17, 0, 5;
    %store/vec4 v0x5a7a81e35b10_0, 0, 5;
    %delay 100, 0;
    %pushi/vec4 18, 0, 5;
    %store/vec4 v0x5a7a81e35b10_0, 0, 5;
    %delay 50, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5a7a81e35a20_0, 0, 1;
    %delay 70, 0;
    %pushi/vec4 17, 0, 5;
    %store/vec4 v0x5a7a81e35b10_0, 0, 5;
    %delay 2000, 0;
    %vpi_call 2 42 "$finish" {0 0 0};
    %end;
