:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x63f6bd632d00 .scope module, "IM_tb" "IM_tb" 2 1;
 .timescale 0 0;
v0x63f6bd645b10_0 .var "address", 4 0;
v0x63f6bd645c00_0 .var "clk", 0 0;
v0x63f6bd645cd0_0 .net "instruction_out", 7 0, v0x63f6bd6458e0_0;  1 drivers
S_0x63f6bd632e90 .scope module, "u_IM" "IM" 2 7, 3 1 0, S_0x63f6bd632d00;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 5 "address";
    .port_info 2 /OUTPUT 8 "instruction_out";
P_0x63f6bd61ee40 .param/l "ADDRESS_WIDTH" 0 3 7, +C4<00000000000000000000000000000101>;
P_0x63f6bd61ee80 .param/l "DATA_WIDTH" 0 3 6, +C4<00000000000000000000000000001000>;
P_0x63f6bd61eec0 .param/l "MEMORY_DEPTH" 0 3 8, +C4<00000000000000000000000000100000>;
v0x63f6bd5f8560_0 .net "address", 4 0, v0x63f6bd645b10_0;  1 drivers
v0x63f6bd645820_0 .net "clk", 0 0, v0x63f6bd645c00_0;  1 drivers
v0x63f6bd6458e0_0 .var "instruction_out", 7 0;
v0x63f6bd6459d0 .array "instructions", 0 31, 7 0;
E_0x63f6bd631480 .event posedge, v0x63f6bd645820_0;
    .scope S_0x63f6bd632e90;
T_0 ;
    %vpi_call 3 17 "$readmemb", "./instruction.mem", v0x63f6bd6459d0, 32'sb00000000000000000000000000000000, 32'sb00000000000000000000000000011111 {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x63f6bd632e90;
T_1 ;
    %wait E_0x63f6bd631480;
    %load/vec4 v0x63f6bd5f8560_0;
    %pad/u 7;
    %ix/vec4 4;
    %load/vec4a v0x63f6bd6459d0, 4;
    %store/vec4 v0x63f6bd6458e0_0, 0, 8;
    %jmp T_1;
    .thread T_1;
    .scope S_0x63f6bd632d00;
T_2 ;
    %vpi_call 2 14 "$dumpfile", "IM.vcd" {0 0 0};
    %vpi_call 2 15 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x63f6bd632d00 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x63f6bd632d00;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x63f6bd645c00_0, 0, 1;
T_3.0 ;
    %delay 25, 0;
    %load/vec4 v0x63f6bd645c00_0;
    %inv;
    %store/vec4 v0x63f6bd645c00_0, 0, 1;
    %jmp T_3.0;
    %end;
    .thread T_3;
    .scope S_0x63f6bd632d00;
T_4 ;
    %pushi/vec4 0, 0, 5;
    %store/vec4 v0x63f6bd645b10_0, 0, 5;
    %delay 50, 0;
    %pushi/vec4 17, 0, 5;
    %store/vec4 v0x63f6bd645b10_0, 0, 5;
    %delay 100, 0;
    %pushi/vec4 18, 0, 5;
    %store/vec4 v0x63f6bd645b10_0, 0, 5;
    %delay 50, 0;
    %delay 2000, 0;
    %vpi_call 2 38 "$finish" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
