v0x5c91d3efc490_0 .net "ACCwrite", 0 0, v0x5c91d3f490d0_0;  1 drivers
v0x5c91d3f48d90_0 .net "clk", 0 0, v0x5c91d3f49250_0;  1 drivers
v0x5c91d3f48e50_0 .net "in", 7 0, v0x5c91d3f49350_0;  1 drivers
v0x5c91d3f48f40_0 .var "out", 7 0;
E_0x5c91d3f354a0 .event posedge, v0x5c91d3f48d90_0;
    .scope S_0x5c91d3f35fd0;
T_0 ;
    %wait E_0x5c91d3f354a0;
    %load/vec4 v0x5c91d3efc490_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x5c91d3f48e50_0;
    %assign/vec4 v0x5c91d3f48f40_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5c91d3f48f40_0;
    %assign/vec4 v0x5c91d3f48f40_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5c91d3f35e40;
T_1 ;
    %vpi_call 2 16 "$dumpfile", "ACC.vcd" {0 0 0};
    %vpi_call 2 17 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5c91d3f35e40 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x5c91d3f35e40;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c91d3f49250_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c91d3f490d0_0, 0, 1;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x5c91d3f49350_0, 0, 8;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5c91d3f490d0_0, 0, 1;
    %delay 45, 0;
    %pushi/vec4 170, 0, 8;
    %store/vec4 v0x5c91d3f49350_0, 0, 8;
    %delay 40, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c91d3f490d0_0, 0, 1;
    %pushi/vec4 129, 0, 8;
    %store/vec4 v0x5c91d3f49350_0, 0, 8;
    %delay 200, 0;
    %end;
    .thread T_2;
    .scope S_0x5c91d3f35e40;
T_3 ;
T_3.0 ;
    %load/vec4 v0x5c91d3f49250_0;
    %inv;
