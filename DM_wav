:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5b7833348c10 .scope module, "DM_tb" "DM_tb" 2 2;
 .timescale 0 0;
v0x5b783335de70_0 .var "address", 4 0;
v0x5b783335df50_0 .var "clk", 0 0;
v0x5b783335e020_0 .var "data_in", 7 0;
v0x5b783335e120_0 .net "data_out", 7 0, v0x5b783335db00_0;  1 drivers
v0x5b783335e1f0_0 .var "memRead", 0 0;
v0x5b783335e2e0_0 .var "memWrite", 0 0;
S_0x5b7833348da0 .scope module, "u_DM" "DM" 2 9, 3 1 0, S_0x5b7833348c10;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "memWrite";
    .port_info 2 /INPUT 1 "memRead";
    .port_info 3 /INPUT 5 "address";
    .port_info 4 /INPUT 8 "data_in";
    .port_info 5 /OUTPUT 8 "data_out";
P_0x5b7833334c50 .param/l "ADDRESS_WIDTH" 0 3 11, +C4<00000000000000000000000000000101>;
P_0x5b7833334c90 .param/l "DATA_WIDTH" 0 3 10, +C4<00000000000000000000000000001000>;
P_0x5b7833334cd0 .param/l "MEMORY_DEPTH" 0 3 12, +C4<00000000000000000000000000100000>;
v0x5b783330d530_0 .net "address", 4 0, v0x5b783335de70_0;  1 drivers
v0x5b783335d890_0 .net "clk", 0 0, v0x5b783335df50_0;  1 drivers
v0x5b783335d950 .array "data", 0 31, 7 0;
v0x5b783335da20_0 .net "data_in", 7 0, v0x5b783335e020_0;  1 drivers
v0x5b783335db00_0 .var "data_out", 7 0;
v0x5b783335dc30_0 .net "memRead", 0 0, v0x5b783335e1f0_0;  1 drivers
v0x5b783335dcf0_0 .net "memWrite", 0 0, v0x5b783335e2e0_0;  1 drivers
E_0x5b7833346680 .event posedge, v0x5b783335d890_0;
    .scope S_0x5b7833348da0;
T_0 ;
    %vpi_call 3 22 "$readmemb", "./data.mem", v0x5b783335d950, 32'sb00000000000000000000000000000000, 32'sb00000000000000000000000000011111 {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x5b7833348da0;
T_1 ;
    %wait E_0x5b7833346680;
    %load/vec4 v0x5b783335dcf0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %load/vec4 v0x5b783335da20_0;
    %load/vec4 v0x5b783330d530_0;
    %pad/u 7;
    %ix/vec4 3;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v0x5b783335d950, 0, 4;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x5b783335dc30_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.2, 8;
    %load/vec4 v0x5b783330d530_0;
    %pad/u 7;
    %ix/vec4 4;
    %load/vec4a v0x5b783335d950, 4;
    %assign/vec4 v0x5b783335db00_0, 0;
T_1.2 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5b7833348c10;
T_2 ;
    %vpi_call 2 19 "$dumpfile", "DM.vcd" {0 0 0};
    %vpi_call 2 20 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5b7833348c10 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x5b7833348c10;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b783335df50_0, 0, 1;
T_3.0 ;
    %delay 25, 0;
    %load/vec4 v0x5b783335df50_0;
    %inv;
    %store/vec4 v0x5b783335df50_0, 0, 1;
    %jmp T_3.0;
    %end;
    .thread T_3;
    .scope S_0x5b7833348c10;
T_4 ;
    %pushi/vec4 0, 0, 5;
    %store/vec4 v0x5b783335de70_0, 0, 5;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b783335e1f0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b783335e2e0_0, 0, 1;
    %pushi/vec4 255, 0, 8;
    %store/vec4 v0x5b783335e020_0, 0, 8;
    %delay 50, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5b783335e2e0_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b783335e2e0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5b783335e1f0_0, 0, 1;
    %delay 50, 0;
    %delay 2000, 0;
    %vpi_call 2 48 "$finish" {0 0 0};
    %end;
    .thread T_4;
