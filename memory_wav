#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x60a8c1160100 .scope module, "memory_tb" "memory_tb" 2 2;
 .timescale -9 -12;
v0x60a8c1176a80_0 .var "address", 4 0;
v0x60a8c1176b60_0 .var "clk", 0 0;
RS_0x71b89aba00d8 .resolv tri, L_0x60a8c1176f80, v0x60a8c1176cd0_0;
v0x60a8c1176c00_0 .net8 "data", 7 0, RS_0x71b89aba00d8;  2 drivers
v0x60a8c1176cd0_0 .var "data_in", 7 0;
v0x60a8c1176d70_0 .var "write_en", 0 0;
S_0x60a8c1160290 .scope module, "u_memory" "memory" 2 10, 3 1 0, S_0x60a8c1160100;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "write_en";
    .port_info 2 /INPUT 5 "address";
    .port_info 3 /INOUT 8 "data";
v0x60a8c114c030_0 .net *"_ivl_1", 0 0, L_0x60a8c1176e60;  1 drivers
o0x71b89aba0048 .functor BUFZ 8, C4<zzzzzzzz>; HiZ drive
; Elide local net with no drivers, v0x60a8c114a650_0 name=_ivl_2
v0x60a8c11764f0_0 .net "address", 4 0, v0x60a8c1176a80_0;  1 drivers
v0x60a8c11765b0_0 .net "clk", 0 0, v0x60a8c1176b60_0;  1 drivers
v0x60a8c1176670_0 .net8 "data", 7 0, RS_0x71b89aba00d8;  alias, 2 drivers
v0x60a8c11767a0_0 .var "data_out", 7 0;
v0x60a8c1176880 .array "mem", 0 31, 7 0;
v0x60a8c1176940_0 .net "write_en", 0 0, v0x60a8c1176d70_0;  1 drivers
E_0x60a8c115d4e0 .event posedge, v0x60a8c11765b0_0;
L_0x60a8c1176e60 .reduce/nor v0x60a8c1176d70_0;
L_0x60a8c1176f80 .functor MUXZ 8, o0x71b89aba0048, v0x60a8c11767a0_0, L_0x60a8c1176e60, C4<>;
    .scope S_0x60a8c1160290;
T_0 ;
    %vpi_call 3 12 "$readmemb", "./instruction.mem", v0x60a8c1176880, 32'sb00000000000000000000000000000000, 32'sb00000000000000000000000000001111 {0 0 0};
    %vpi_call 3 13 "$readmemb", "./data.mem", v0x60a8c1176880, 32'sb00000000000000000000000000010000, 32'sb00000000000000000000000000011111 {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x60a8c1160290;
T_1 ;
    %wait E_0x60a8c115d4e0;
    %load/vec4 v0x60a8c1176940_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %load/vec4 v0x60a8c1176670_0;
    %load/vec4 v0x60a8c11764f0_0;
    %pad/u 7;
    %ix/vec4 3;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v0x60a8c1176880, 0, 4;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x60a8c11764f0_0;
    %pad/u 7;
    %ix/vec4 4;
    %load/vec4a v0x60a8c1176880, 4;
    %store/vec4 v0x60a8c11767a0_0, 0, 8;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x60a8c1160290;
T_2 ;
    %wait E_0x60a8c115d4e0;
    %vpi_call 3 31 "$writememb", "./data.mem", v0x60a8c1176880, 32'sb00000000000000000000000000010000, 32'sb00000000000000000000000000011111 {0 0 0};
    %jmp T_2;
    .thread T_2;
    .scope S_0x60a8c1160100;
T_3 ;
    %vpi_call 2 20 "$dumpfile", "memory.vcd" {0 0 0};
    %vpi_call 2 21 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x60a8c1160100 {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x60a8c1160100;
T_4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60a8c1176b60_0, 0, 1;
T_4.0 ;
    %delay 25000, 0;
    %load/vec4 v0x60a8c1176b60_0;
    %inv;
    %store/vec4 v0x60a8c1176b60_0, 0, 1;
    %jmp T_4.0;
    %end;
    .thread T_4;
    .scope S_0x60a8c1160100;
T_5 ;
    %pushi/vec4 0, 0, 5;
    %store/vec4 v0x60a8c1176a80_0, 0, 5;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x60a8c1176cd0_0, 0, 8;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60a8c1176d70_0, 0, 1;
    %delay 50000, 0;
    %pushi/vec4 17, 0, 5;
    %store/vec4 v0x60a8c1176a80_0, 0, 5;
    %pushi/vec4 255, 0, 8;
    %store/vec4 v0x60a8c1176cd0_0, 0, 8;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x60a8c1176d70_0, 0, 1;
    %delay 50000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60a8c1176d70_0, 0, 1;
    %delay 100000, 0;
    %pushi/vec4 17, 0, 5;
    %store/vec4 v0x60a8c1176a80_0, 0, 5;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60a8c1176d70_0, 0, 1;
    %delay 50000, 0;
    %delay 2000000, 0;
    %vpi_call 2 53 "$finish" {0 0 0};
    %end;
    .thread T_5;
